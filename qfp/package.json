{
  "name": "quantum-fragility-playground",
  "version": "2.0.0",
  "description": "virtual quantum lab i built for HackXAmrita 2.0",
  "author": "NVNAGATHARUN",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "npm --prefix ../quantum-fragility-playground run server",
    "server:build": "npm --prefix ../quantum-fragility-playground run server:build",
    "server:start": "npm --prefix ../quantum-fragility-playground run server:start",
    "build:full": "npm run server:build && npm run build",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\""
  },
